{
  "name": "vscode-export-for-ai-chat",
  "version": "0.0.1",
  "type": "module",
  "description": "Copies the current fileâ€™s content to the clipboard in a format that is suitable for pasting into an AI chat.",
  "keywords": [
    "code-extension",
    "extension",
    "keybinding",
    "keybindings",
    "keymap",
    "keymaps",
    "reset",
    "vsce",
    "vscode",
    "vscode-extension",
    "vscode-keybindings",
    "vsix"
  ],
  "author": "Jaid <jaid.jsx@gmail.com> (https://github.com/Jaid)",
  "homepage": "https://github.com/Jaid/vscode-export-for-ai-chat",
  "funding": "https://github.com/sponsors/jaid",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaid/vscode-export-for-ai-chat"
  },
  "main": "./extension.js",
  "scripts": {
    "build": "tsx src/workflows/build/build.ts",
    "buildExtension": "tsx src/workflows/buildExtension/build.ts",
    "buildHtml": "tsx src/workflows/buildHtml/build.ts"
  },
  "dependencies": {
    "zeug": "jaid/zeug#tsc"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@types/node": "^20.11.20",
    "browserslist-config-jaid-universal": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-jaid": "^8.20.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.1",
    "handlebars": "^4.7.8",
    "key-counter": "^1.0.4",
    "lodash-es": "^4.17.21",
    "prevent-start": "^1.0.5",
    "read-file-json": "^1.1.0",
    "read-file-string": "^1.1.2",
    "read-file-yaml": "^2.0.0",
    "showdown": "^2.1.0",
    "super-regex": "^0.3.0",
    "tsconfig-jaid": "^6.1.1",
    "tsconfig-jaid-node": "^6.1.1",
    "tsx": "^4.7.1",
    "type-fest": "^4.10.3",
    "typescript": "^5.3.3",
    "wireit": "^0.14.4",
    "yaml": "^2.4.0"
  },
  "categories": [
    "Education",
    "Other"
  ],
  "contributes": {
    "commands": [{
      "command": "export-for-ai-chat-copy.copyCode",
      "title": "Copy as Markdown Code"
    }],
    "menus": {
      "editor/context": [{
          "command": "export-for-ai-chat-copy.copyCode",
          "when": "editorTextFocus",
          "group": "z_commands"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.81.0"
  }
}
