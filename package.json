{
  "name": "vscode-export-for-ai-chat",
  "displayName": "Export for AI chat",
  "version": "0.0.1",
  "type": "module",
  "description": "Copies the current fileâ€™s content to the clipboard in a format that is suitable for pasting into an AI chat.",
  "keywords": [
    "code-extension",
    "extension",
    "vsce",
    "vscode",
    "vscode-extension",
    "vsix",
    "ai",
    "ai-chat",
    "prompt",
    "ai-prompt",
    "chat",
    "prompt-generator",
    "prompt-generation",
    "ai-help",
    "ai-assistant",
    "chat-assistant"
  ],
  "author": "Jaid <jaid.jsx@gmail.com> (https://github.com/Jaid)",
  "homepage": "https://github.com/Jaid/vscode-export-for-ai-chat",
  "funding": "https://github.com/sponsors/jaid",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaid/vscode-export-for-ai-chat"
  },
  "categories": [
    "Education",
    "Other"
  ],
  "scripts": {
    "build": "fse emptyDir out/bun && bun x/build.ts",
    "build_test": "fse emptyDir out/test && bun build test/code/*.ts --outdir out/test --target node --format cjs --entry-naming [name].test.cjs --external vscode",
    "clean": "fse remove out && fse remove temp && fse remove .vscode-test",
    "pack": "fse emptyDir out/vsix && NODE_ENV=production bun run build && cp license.txt out/bun && cd out/bun && vsce package --out ../vsix/$npm_package_name-v$npm_package_version.vsix",
    "start": "bun run build && code --verbose --disable-extensions --extensionDevelopmentPath=$npm_config_local_prefix/out/bun",
    "test": "bun test ./test",
    "test_integration": "bun run build && bun run build_test && vscode-test",
    "use": "bun run pack && code --install-extension out/vsix/$npm_package_name-v$npm_package_version.vsix"
  },
  "dependencies": {
    "js-tiktoken": "^1.0.21",
    "zeug": "^2.1.0"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@atao60/fse-cli": "^0.1.9",
    "@types/bun": "^1.2.23",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.6.2",
    "@types/vscode": "^1.104.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "browserslist-config-jaid-universal": "^3.0.0",
    "eslint": "^9.37.0",
    "eslint-config-jaid": "^18.1.0",
    "fs-extra": "^11.3.2",
    "globby": "^15.0.0",
    "handlebars": "^4.7.8",
    "key-counter": "^1.0.4",
    "lodash-es": "^4.17.21",
    "mocha": "^11.7.4",
    "more-types": "^0.6.0",
    "prevent-start": "^1.0.5",
    "read-file-json": "^1.1.0",
    "read-file-string": "^1.1.2",
    "read-file-yaml": "^2.0.0",
    "showdown": "^2.1.0",
    "super-regex": "^1.0.0",
    "tsconfig-jaid-node": "^8.1.0",
    "type-fest": "^5.0.1",
    "typescript": "5.9.3",
    "yaml": "^2.8.1"
  },
  "engines": {
    "vscode": "^1.85.0"
  }
}